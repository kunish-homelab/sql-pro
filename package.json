{
  "name": "sql-pro-monorepo",
  "version": "1.9.0",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "description": "SQL Pro Monorepo - Professional SQLite database manager",
  "author": {
    "name": "kunish",
    "email": "kunish.butt@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "pnpm --filter @sqlpro/app build",
    "build:mac": "pnpm --filter @sqlpro/app build:mac",
    "build:win": "pnpm --filter @sqlpro/app build:win",
    "build:linux": "pnpm --filter @sqlpro/app build:linux",
    "dev": "pnpm --filter @sqlpro/app dev",
    "dev:mock": "pnpm --filter @sqlpro/app dev:mock",
    "docs:build": "pnpm --filter @sqlpro/docs build",
    "docs:dev": "pnpm --filter @sqlpro/docs dev",
    "docs:preview": "pnpm --filter @sqlpro/docs preview",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "prepare": "husky",
    "test": "pnpm --filter @sqlpro/app test",
    "test:coverage": "pnpm --filter @sqlpro/app test:coverage",
    "test:run": "pnpm --filter @sqlpro/app test:run",
    "typecheck": "pnpm --filter @sqlpro/app typecheck"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@electron/notarize": "^3.1.1",
    "@eslint-react/eslint-plugin": "^2.5.0",
    "@nx/js": "^22.3.3",
    "@nx/vite": "^22.3.3",
    "@nx/workspace": "^22.3.3",
    "@typescript/native-preview": "7.0.0-dev.20260103.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nx": "^22.3.3",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "typescript": "^5.9.3"
  }
}
