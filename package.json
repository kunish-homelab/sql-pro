{
  "name": "sql-pro",
  "version": "1.6.0",
  "packageManager": "pnpm@10.26.2",
  "description": "Professional SQLite database manager with SQLCipher support and diff preview",
  "author": {
    "name": "kunish",
    "email": "kunish.butt@gmail.com"
  },
  "license": "MIT",
  "main": "./out/main/index.js",
  "scripts": {
    "build": "electron-vite build",
    "build:icons": "fish resources/generate-icons.fish",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "dev": "electron-vite dev",
    "dev:mock": "VITE_MOCK_MODE=true electron-vite dev",
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "format": "prettier --write .",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --fix .",
    "prepare": "husky",
    "preview": "electron-vite preview",
    "screenshots": "tsx scripts/capture-screenshots.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "typecheck": "pnpm run typecheck:main && pnpm run typecheck:preload && pnpm run typecheck:renderer",
    "typecheck:main": "tsc --noEmit -p src/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p src/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p src/renderer/tsconfig.json"
  },
