{
  "name": "sql-pro",
  "version": "1.0.0",
  "packageManager": "pnpm@10.26.2",
  "description": "Professional SQLite database manager with SQLCipher support and diff preview",
  "author": {
    "name": "kunish",
    "email": "kunish.butt@gmail.com"
  },
  "license": "MIT",
  "main": "./out/main/index.js",
  "scripts": {
    "build": "electron-vite build",
    "build:icons": "fish resources/generate-icons.fish",
    "build:linux": "pnpm run build && electron-builder --linux",
    "build:mac": "pnpm run build && electron-builder --mac",
    "build:unpack": "pnpm run build && electron-builder --dir",
    "build:win": "pnpm run build && electron-builder --win",
    "dev": "electron-vite dev",
    "format": "prettier --write .",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint --fix .",
    "prepare": "husky",
    "preview": "electron-vite preview",
    "typecheck": "pnpm run typecheck:main && pnpm run typecheck:preload && pnpm run typecheck:renderer",
    "typecheck:main": "tsc --noEmit -p src/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p src/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p src/renderer/tsconfig.json"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/db": "^0.5.16",
    "@tanstack/query-core": "^5.90.12",
    "@tanstack/query-db-collection": "^1.0.12",
    "@tanstack/react-db": "^0.1.60",
    "@tanstack/react-devtools": "^0.9.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.143.11",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.13",
    "better-sqlite3-multiple-ciphers": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@electron/notarize": "^3.1.1",
    "@electron/rebuild": "^4.0.2",
    "@eslint-react/eslint-plugin": "^2.4.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-router-devtools": "^1.143.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-vite": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
